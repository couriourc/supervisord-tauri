{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "store:default",
    "sql:default",
    "sql:allow-execute",
    "clipboard-manager:default",
    "clipboard-manager:allow-write-text"
  ]
}
